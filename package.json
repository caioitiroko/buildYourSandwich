{
  "name": "buildYourSandwich",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "android-bundle": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res/",
    "build-android": "yarn run android-bundle && cd android && ./gradlew -p android buildYourSandwichRelease && cd -",
    "ios": "react-native run-ios",
    "ios-bundle": "react-native bundle --entry-file index.js --platform ios --dev false --bundle-output ios/main.jsbundle --assets-dest ios",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "lint": "eslint ./src",
    "log-android": "react-native log-android",
    "log-ios": "react-native log-ios",
    "postinstall": "node ./scripts/android-release-gradle-fix.js",
    "test": "jest"
  },
  "pre-push": [
    "test",
    "lint"
  ],
  "dependencies": {
    "babel-eslint": "^10.0.1",
    "prop-types": "^15.7.2",
    "ramda": "^0.26.1",
    "react": "16.8.3",
    "react-native": "0.59.3",
    "react-native-router-flux": "^4.0.6",
    "react-pure-lifecycle": "^3.0.0",
    "react-redux": "^6.0.1",
    "recompose": "^0.30.0",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "redux-saga": "^1.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/runtime": "^7.4.3",
    "babel-jest": "^24.7.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "jest": "^24.7.1",
    "metro-react-native-babel-preset": "^0.53.1",
    "pre-push": "^0.1.1",
    "react-test-renderer": "16.8.3"
  },
  "jest": {
    "preset": "react-native"
  }
}
